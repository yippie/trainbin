<h1>Listing prototypes</h1>

<div class="table-responsive">
<table class="table table-bordered table-condensed table-hover">
  <thead>
    <tr>
      <th><%= Prototype.human_attribute_name("name") %></th>
      <th><%= Prototype.human_attribute_name("enginetype") %></th>
      <th><%= Prototype.human_attribute_name("manufacturer") %></th>
      <th><%= Prototype.human_attribute_name("model") %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prototypes.each do |prototype| %>
      <tr>
        <td><%= prototype.name %></td>
        <td><%= prototype.enginetype %></td>
        <td><%= prototype.manufacturer %></td>
        <td><%= prototype.model %></td>
        <td><%= link_to 'Show', prototype %></td>
        <td><%= link_to 'Edit', edit_prototype_path(prototype) %></td>
        <td><%= link_to 'Destroy', prototype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<%= link_to 'New Prototype', new_prototype_path %>
